doctype html
html
    head
        link(rel='stylesheet', href='../css/home.css')
    body
        div.hero-area
            #hero.row
                div.hero-main
                    .hero-img
                        img(src='data:image/jpeg;base64,')
                    -let message = "User Name"
                    -let n = "kappa@yahoo.com"
                    -let m= "check"
                    .hero-text=message
                      .hero-text-detail="E-mail : " + n
                      .hero-text-detail="Owner : " + m
                .hero-social-buttons.col-xs-12.text-center
                    a.btn.btn-kn-cta.btn-twitter(href="https://github.com/didianca") GitHub
                    a.btn.btn-kn-cta.btn-LinkedIn(href="https://codestats.net/users/didi") Code::Stats
                div.hero-profile
                .hero-footer
                div
        div.nav-header
            -let a= "2019"
            div.nav-menu="Member since " + a
        div.banner.sticky-nav  Something I might wanna put here
        div.three-boxes
            div.child-box(data-aos="fade-right"
                data-aos-easing="ease-out-cubic"
                data-aos-duration="2000")
                a.btn.btn-kn-cta.user-actions(href="./register-pet.pug") Register Pet
            div.child-box
                a.btn.btn-kn-cta.user-actions(href="./rental.css") Rent Room
            div.child-box(data-aos="fade-left"
                data-aos-easing="ease-out-cubic"
                data-aos-duration="2000")
                a.btn.btn-kn-cta.user-actions(href="./become-owner.css") Become Owner
        div.profile-container My Pets
            -let myPets =["Luna","Cassie"]
            each val in myPets
                div(class="profile-child", data-aos="flip-up"
                    data-aos-easing="ease-out-cubic"
                    data-aos-duration="2000")
                    p.profile-child-paragraph=val
        div.recommendations
            each val in [1, 2, 3, 4, 5,1,1, 2]
                div.recommendation(data-aos="fade-up", data-aos-easing="ease-out-cubic"
                    data-aos-duration="1500" )=val